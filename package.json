{
  "name": "gm-fabric-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dateformat": "~2.0.0",
    "filesize": "~3.5.6",
    "humps": "~2.0.0",
    "jquery": "~3.2.1",
    "jumpstate": "~2.2.2",
    "lodash": "~4.17.4",
    "ms": "~1.0.0",
    "pondjs": "~0.8.5",
    "prop-types": "~15.5.8",
    "react": "~15.5.4",
    "react-collapse": "~2.3.3",
    "react-dom": "~15.4.2",
    "react-height": "~2.2.0",
    "react-input-range": "~1.1.2",
    "react-json-inspector": "~7.0.0",
    "react-motion": "~0.4.7",
    "react-redux": "~5.0.3",
    "react-router": "~3.0.0",
    "react-router-redux": "~4.0.8",
    "react-sparklines": "~1.6.0",
    "react-timeseries-charts": "~0.12.4",
    "recharts": "~0.21.2",
    "redux": "~3.6.0",
    "redux-devtools": "~3.3.2",
    "redux-logger": "~3.0.0",
    "reselect": "~3.0.0",
    "uikit": "~3.0.0-beta.21"
  },
  "devDependencies": {
    "enzyme": "~2.7.1",
    "jest-junit-reporter": "~1.1.0",
    "less": "~2.7.2",
    "npm-run-all": "~4.0.2",
    "npm-watch": "~0.1.8",
    "react-a11y": "~0.3.3",
    "react-addons-test-utils": "~15.4.2",
    "react-scripts": "~0.9.5",
    "stylelint": "~7.10.1",
    "stylelint-config-standard": "~16.0.0"
  },
  "watch": {
    "build-css": {
      "patterns": [
        "src/style",
        "src/style/components/",
        "src/style/views"
      ],
      "extensions": "less"
    }
  },
  "scripts": {
    "build-css": "lessc -ru src/style/index.less src/style/index.css",
    "watch": "npm-watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch start-js",
    "lint-css": "stylelint 'src/style/**/*.less'",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:9990",
  "homepage": "http://localhost:8888/gmadmin"
}
